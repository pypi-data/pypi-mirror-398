params {
    modules_testdata_base_path = 'https://raw.githubusercontent.com/nf-core/test-datasets/modules/data/'
    db_host = 'localhost'
    db_port = 5432
    db_name = 'vcf_test'
    db_user = 'postgres'
    db_schema = 'public'
    batch_size = '10000'
    module_args = ''
}

profiles {
    docker {
        docker.enabled = true
    }
    singularity {
        singularity.enabled = true
    }
}

process {
    withName: 'VCFPGLOADER' {
        ext.args = ''
        ext.batch_size = '10000'
    }
}
