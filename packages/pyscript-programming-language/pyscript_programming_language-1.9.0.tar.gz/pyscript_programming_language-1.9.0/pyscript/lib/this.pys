try {
    file = open(pyimport('os').path.join(require('sys').pyscript_path, 'this.py'), 'r')
    local = {}

    try {
        pyimport('builtins').exec(file.read(), {}, local)
        s = local['s']
    }

    finally {
        file.close()
        del file, local
    }
}

catch {
    s = "Uh, something's wrong!"
    print(s)
}