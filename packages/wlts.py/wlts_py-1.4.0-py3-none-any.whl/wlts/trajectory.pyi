import geopandas as gpd
import pandas as pd
from .utils import Utils as Utils
from typing import Any

class Trajectory(dict):
    def __init__(self, data: dict[str, Any]) -> None: ...
    @property
    def trajectory(self, as_date: bool = False, index: int = 1, fmt: str = '') -> list[dict[str, Any]]: ...
    @property
    def query(self, as_date: bool = False, fmt: str = '') -> dict[str, Any]: ...
    def df(self, **options) -> pd.DataFrame: ...
    def geodf(self, **options) -> gpd.GeoDataFrame: ...
