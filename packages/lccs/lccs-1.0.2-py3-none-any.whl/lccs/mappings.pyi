from .classes import ClassificationSystemClass as ClassificationSystemClass
from .utils import Utils as Utils

class MappingGroup(dict):
    def __init__(self, data: dict, validate: bool = False) -> None: ...
    @property
    def mappings(self) -> list['Mapping']: ...

class Mapping(dict):
    def __init__(self, data: dict, validate: bool = False) -> None: ...
    @property
    def degree_of_similarity(self) -> float | None: ...
    @property
    def description(self) -> str | None: ...
    @property
    def link(self) -> list[dict]: ...
    @property
    def source_class(self) -> ClassificationSystemClass | None: ...
    @property
    def target_class(self) -> ClassificationSystemClass | None: ...
    @property
    def source_class_id(self) -> int | None: ...
    @property
    def target_class_id(self) -> int | None: ...
