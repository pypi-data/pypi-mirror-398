  "business_rules": [
    "order_id must be unique and follow sequential numbering pattern",
    "customer.id must be unique per customer",
    "subtotal = sum of (quantity \u00d7 unit_price) for all items",
    "tax_amount = subtotal \u00d7 tax_rate (rounded to 2 decimal places)",
    "total = subtotal + tax_amount",
    "created_at must be <= updated_at",
    "shipped_at must be >= created_at when not null",
    "shipped_at must be <= updated_at when not null",
    "shipped_at is null when status is 'processing'",
    "shipped_at has a value when status is 'shipped' or 'delivered'",
    "updated_at equals created_at for newly created orders",
    "status must be one of: 'processing', 'shipped', 'delivered' (and possibly others)",
    "items array must contain at least one item",
    "quantity must be a positive integer",
    "unit_price, subtotal, tax_amount, and total must be positive decimals with 2 decimal places",
    "tax_rate is a decimal representing percentage (e.g., 0.0825 = 8.25%)",
    "tax_rate varies by state (likely based on customer.address.state)",
    "All monetary values are in USD (implied)"
  ],
