{
  "fit-fit": {
    "lineWidth": 2,
    "colour": [1, 0, 0]
  },
  "fit-baseline": {
    "lineWidth": 1,
    "colour": [0, 0.5, 0.5]
  },
  "fit-residual": {
    "lineWidth": 1,
    "colour": [0.2, 0.2, 0.2]
  },
  "raw-concentrations": {
    "cmap": "hot",
    "displayRange": ["0.0", "median + 3 * std"]
  },
  "conc-internal": {
    "cmap": "hot",
    "displayRange": ["0.0", "median + 3 * std"]
  },
  "conc-molarity": {
    "cmap": "hot",
    "displayRange": ["0.0", "median + 3 * std"]
  },
  "conc-molality": {
    "cmap": "hot",
    "displayRange": ["0.0", "median + 3 * std"]
  },
  "sd": {
    "cmap": "cool",
    "clippingRange": ["0.0", "max"],
    "displayRange": ["median - 2 * std", "median + 2 * std"],
    "linkLowRanges": false
  },
  "SNR": {
    "cmap": "red",
    "clippingRange": ["0.0", "max"],
    "displayRange": ["median - 2 * std", "median + 2 * std"],
    "linkLowRanges": false
  },
  "FWHM": {
    "cmap": "blue",
    "clippingRange": ["0.0", "max"],
    "displayRange": ["median - 2 * std", "median + 2 * std"],
    "linkLowRanges": false
  }
}