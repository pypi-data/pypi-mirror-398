from easy_start_chanjet.zkhb.http import Api

async def main():
    # åˆ›å»ºAPIå®ä¾‹
    api = Api(base_url="http://127.0.0.1/")
    
    # å¼‚æ­¥è°ƒç”¨æ¥å£è·å–å®é™…ä»˜æ¬¾é¡¹ç›®åˆ—è¡¨
