{
  "openapi": "3.0.0",
  "info": {
    "title": "Openai Primitives API (Layer 1)",
    "version": "0.1.0",
    "description": "Primitive helper endpoints for openai"
  },
  "servers": [
    {
      "url": "http://localhost:8000",
      "description": "Local development server"
    }
  ],
  "paths": {
    "/primitives/openai/list-chat-completions": {
      "get": {
        "operationId": "list_chat_completions_structured",
        "summary": "list_chat_completions_structured - Layer 1 Primitive",
        "description": "List stored Chat Completions. Only Chat Completions that have been stored\nwith the `store` parameter set to `true` will be returned.",
        "tags": [
          "openai-primitives"
        ],
        "parameters": [
          {
            "name": "model",
            "in": "query",
            "required": false,
            "description": "The model used to generate the Chat Completions.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "metadata",
            "in": "query",
            "required": false,
            "description": "A list of metadata keys to filter the Chat Completions by. Example:\n\n`metadata[key1]=value1&metadata[key2]=value2`",
            "schema": {
              "type": "object"
            }
          },
          {
            "name": "after",
            "in": "query",
            "required": false,
            "description": "Identifier for the last chat completion from the previous pagination request.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "description": "Number of Chat Completions to retrieve.",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "order",
            "in": "query",
            "required": false,
            "description": "Sort order for Chat Completions by timestamp. Use `asc` for ascending order or `desc` for descending order. Defaults to `asc`.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/primitives/openai/create-chat-completion": {
      "post": {
        "operationId": "create_chat_completion_structured",
        "summary": "create_chat_completion_structured - Layer 1 Primitive",
        "description": "**Starting a new project?** We recommend trying [Responses](https://platform.openai.com/docs/api-reference/responses) \nto take advantage of the latest OpenAI platform features. Compare\n[Chat Completions with Responses](https://platform.openai.com/docs/guides/responses-vs-chat-completions?api-mode=responses).\n\n---\n\nCreates a model response for the given chat conversation. Learn more in the\n[text generation](https://platform.openai.com/docs/guides/text-generation), [vision](https://platform.openai.com/docs/guides/vision),\nand [audio](https://platform.openai.com/docs/guides/audio) guides.\n\nParameter support can differ depending on the model used to generate the\nresponse, particularly for newer reasoning models. Parameters that are only\nsupported for reasoning models are noted below. For the current state of \nunsupported parameters in reasoning models, \n[refer to the reasoning guide](https://platform.openai.com/docs/guides/reasoning).",
        "tags": [
          "openai-primitives"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/primitives/openai/get-chat-completion": {
      "get": {
        "operationId": "get_chat_completion_structured",
        "summary": "get_chat_completion_structured - Layer 1 Primitive",
        "description": "Get a stored chat completion. Only Chat Completions that have been created\nwith the `store` parameter set to `true` will be returned.",
        "tags": [
          "openai-primitives"
        ],
        "parameters": [
          {
            "name": "completion_id",
            "in": "query",
            "required": true,
            "description": "The ID of the chat completion to retrieve.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/primitives/openai/update-chat-completion": {
      "post": {
        "operationId": "update_chat_completion_structured",
        "summary": "update_chat_completion_structured - Layer 1 Primitive",
        "description": "Modify a stored chat completion. Only Chat Completions that have been\ncreated with the `store` parameter set to `true` can be modified. Currently,\nthe only supported modification is to update the `metadata` field.",
        "tags": [
          "openai-primitives"
        ],
        "parameters": [
          {
            "name": "completion_id",
            "in": "query",
            "required": true,
            "description": "The ID of the chat completion to update.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/primitives/openai/delete-chat-completion": {
      "get": {
        "operationId": "delete_chat_completion_structured",
        "summary": "delete_chat_completion_structured - Layer 1 Primitive",
        "description": "Delete a stored chat completion. Only Chat Completions that have been\ncreated with the `store` parameter set to `true` can be deleted.",
        "tags": [
          "openai-primitives"
        ],
        "parameters": [
          {
            "name": "completion_id",
            "in": "query",
            "required": true,
            "description": "The ID of the chat completion to delete.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/primitives/openai/get-chat-completion-messages": {
      "get": {
        "operationId": "get_chat_completion_messages_structured",
        "summary": "get_chat_completion_messages_structured - Layer 1 Primitive",
        "description": "Get the messages in a stored chat completion. Only Chat Completions that\nhave been created with the `store` parameter set to `true` will be\nreturned.",
        "tags": [
          "openai-primitives"
        ],
        "parameters": [
          {
            "name": "completion_id",
            "in": "query",
            "required": true,
            "description": "The ID of the chat completion to retrieve messages from.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "after",
            "in": "query",
            "required": false,
            "description": "Identifier for the last message from the previous pagination request.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "description": "Number of messages to retrieve.",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "order",
            "in": "query",
            "required": false,
            "description": "Sort order for messages by timestamp. Use `asc` for ascending order or `desc` for descending order. Defaults to `asc`.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/primitives/openai/create-embedding": {
      "post": {
        "operationId": "create_embedding_structured",
        "summary": "create_embedding_structured - Layer 1 Primitive",
        "description": "Creates an embedding vector representing the input text.",
        "tags": [
          "openai-primitives"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
  "components": {}
}
