<?xml version='1.0' encoding='UTF-8' ?>
<odoo>
    <record
        id="website_sale_extra_infos.product_template_only_form_view"
        model="ir.ui.view"
    >
        <field name="name">product.template.form.inherit.duration</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='barcode']" position="after">
                <field name="duration" />
                <field name="needs_photo" />
            </xpath>
        </field>
    </record>
</odoo>
