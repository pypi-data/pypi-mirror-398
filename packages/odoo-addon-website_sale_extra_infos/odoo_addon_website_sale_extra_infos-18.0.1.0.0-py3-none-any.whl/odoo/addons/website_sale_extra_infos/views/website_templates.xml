<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="extra_info" model="ir.ui.view">
        <field name="name">Checkout Extra Korporation Uri Info</field>
        <field name="type">qweb</field>
        <field name="key">website_sale.extra_info</field>
        <field name="website_id" ref="website.default_website" />
        <field name="arch" type="xml">
            <t
                name="Checkout Extra Info"
                active="False"
                t-name="website_sale.extra_info"
            >
                <t t-call="website_sale.checkout_layout">
                    <t t-set="show_navigation_button" t-value="False" />
                    <t t-set="redirect" t-valuef="/shop/extra_info" />
                    <t t-set="oe_structure">
                        <!-- This is the drag-and-drop area for website building blocs at the end of
                        each
                     checkout page. This is append at the of the page in `checkout_layout`. The
                     templates created in the database to store blocs are hooked using XPath on the
                     `oe_struture` element ID. Therefore, we can't use dynamic IDs (like with
                     t-att-id) and each template needs to define a div element. -->
                        <div
                            class="oe_structure"
                            id="oe_structure_website_sale_extra_info_1"
                        />
                    </t>

                    <h3 class="mb-4">Weitere Informationen</h3>
                    <section
                        class="s_website_form o_colored_level"
                        data-vcss="001"
                        data-snippet="s_website_form"
                    >
                        <div class="container">
                            <form
                                action="/website/form/"
                                method="post"
                                enctype="multipart/form-data"
                                class="o_mark_required s_website_form_no_recaptcha"
                                data-mark="*"
                                data-force_action="shop.sale.order"
                                data-model_name="sale.order"
                                data-success-mode="redirect"
                                data-success-page="/shop/payment"
                                hide-change-model="true"
                            >
                                <div class="s_website_form_rows s_col_no_bgcolor row">
                                    <div
                                        data-name="Datum Beginn"
                                        class="s_website_form_field mb-3 col-12 s_website_form_custom"
                                        data-type="date"
                                        data-translated-name="Benutzerdefinierter Text"
                                    >
                                        <div
                                            class="row s_col_no_resize s_col_no_bgcolor"
                                        >
                                            <label
                                                class="col-form-label col-sm-auto s_website_form_label"
                                                style="width: 200px"
                                                for="o5koaivn76tr"
                                            >
                                                <span
                                                    class="s_website_form_label_content"
                                                >Datum Beginn</span>
                                            </label>
                                            <div class="col-sm">
                                                <div
                                                    class="s_website_form_date input-group date"
                                                >
                                                    <input
                                                        type="text"
                                                        class="form-control datetimepicker-input s_website_form_input"
                                                        name="date_from"
                                                        value=""
                                                        placeholder=""
                                                        id="o5koaivn76tr"
                                                    />
                                                    <div
                                                        class="input-group-text o_input_group_date_icon cursor-pointer"
                                                    >
                                                        <i class="fa fa-calendar" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div
                                        data-name="Region"
                                        class="s_website_form_field mb-3 col-12 s_website_form_custom"
                                        data-type="many2one"
                                        data-translated-name="region"
                                    >
                                        <div
                                            class="row s_col_no_resize s_col_no_bgcolor"
                                        >
                                            <label
                                                class="col-form-label col-sm-auto s_website_form_label"
                                                style="width: 200px"
                                                for="osx5r9oc5lar"
                                            >
                                                <span
                                                    class="s_website_form_label_content"
                                                >Strahlnerregion</span>
                                            </label>
                                            <div class="col-sm">
                                                <select
                                                    class="form-select s_website_form_input"
                                                    name="region"
                                                    id="osx5r9oc5lar"
                                                    required="required"
                                                >
                                                    <option
                                                        value=""
                                                    >-- Please select --</option>
                                                    <option
                                                        id="osx5r9oc5lar0"
                                                        value="galp"
                                                    >1. Göscheneralp/Voralp</option>
                                                    <option
                                                        id="osx5r9oc5lar1"
                                                        value="fell"
                                                    >2.
                                                        Fellital</option>
                                                    <option
                                                        id="osx5r9oc5lar2"
                                                        value="made"
                                                    >3.
                                                        Maderanertal/Etzlital/Brunnital</option>
                                                    <option
                                                        id="osx5r9oc5lar3"
                                                        value="rest"
                                                    >4.
                                                        Übriges Strahlnergebiet Korporation Uri</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div
                                        data-name="Geburtstag"
                                        class="s_website_form_field mb-3 col-12 s_website_form_custom"
                                        data-type="date"
                                        data-translated-name="Geburtstag"
                                    >
                                        <div
                                            class="row s_col_no_resize s_col_no_bgcolor"
                                        >
                                            <label
                                                class="col-form-label col-sm-auto s_website_form_label"
                                                style="width: 200px"
                                                for="o5koaivn76tr"
                                            >
                                                <span
                                                    class="s_website_form_label_content"
                                                >Geburtstag</span>
                                            </label>
                                            <div class="col-sm">
                                                <div
                                                    class="s_website_form_date input-group date"
                                                >
                                                    <input
                                                        type="text"
                                                        class="form-control datetimepicker-input s_website_form_input"
                                                        name="birthdate"
                                                        value=""
                                                        placeholder=""
                                                        id="o5koaivn76tr"
                                                    />
                                                    <div
                                                        class="input-group-text o_input_group_date_icon cursor-pointer"
                                                    >
                                                        <i class="fa fa-calendar" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div
                                        data-name="Field"
                                        class="s_website_form_field mb-3 col-12 s_website_form_custom"
                                        data-type="binary"
                                        data-translated-name="Feld"
                                    >
                                        <div
                                            class="row s_col_no_resize s_col_no_bgcolor"
                                        >
                                            <label
                                                class="col-sm-auto s_website_form_label"
                                                style="width: 200px"
                                                for="o9yrctzinfhn"
                                            >
                                                <span
                                                    class="s_website_form_label_content"
                                                >Foto (obligatorisch für Jahrespatentbesitzer)</span>
                                            </label>
                                            <div class="col-sm">
                                                <input
                                                    type="file"
                                                    class="form-control s_website_form_input"
                                                    name="photo"
                                                    id="o9yrctzinfhn"
                                                    data-max-files-number="1"
                                                    data-max-file-size="1"
                                                />
                                            </div>
                                        </div>
                                    </div>
                                    <div
                                        data-name="Field"
                                        class="s_website_form_field mb-3 col-12 s_website_form_custom s_website_form_required"
                                        data-type="boolean"
                                        data-translated-name="Feld"
                                    >
                                        <div
                                            class="row s_col_no_resize s_col_no_bgcolor"
                                        >
                                            <label
                                                class="col-sm-auto s_website_form_label"
                                                style="width: 200px"
                                                for="orj8inzybxy"
                                            >
                                                <span
                                                    class="s_website_form_label_content"
                                                >Ich habe eine Haftpflichtversicherung</span>
                                                <span
                                                    class="s_website_form_mark"
                                                > *</span>
                                            </label>
                                            <div class="col-sm">
                                                <div class="form-check">
                                                    <input
                                                        type="checkbox"
                                                        value="Yes"
                                                        class="s_website_form_input form-check-input"
                                                        name="liability_insurance"
                                                        id="orj8inzybxy"
                                                        required=""
                                                    />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div
                                        data-name="Field"
                                        class="s_website_form_field mb-3 col-12 s_website_form_custom s_website_form_required"
                                        data-type="boolean"
                                        data-translated-name="Feld"
                                    >
                                        <div
                                            class="row s_col_no_resize s_col_no_bgcolor"
                                        >
                                            <label
                                                class="col-sm-auto s_website_form_label"
                                                style="width: 200px"
                                                for="oc8pzs7wgrmm"
                                            >
                                                <span
                                                    class="s_website_form_label_content"
                                                >Ich habe den Ehrencodex gelesen</span>
                                                <span
                                                    class="s_website_form_mark"
                                                > *</span>
                                            </label>
                                            <div class="col-sm">
                                                <div class="form-check">
                                                    <input
                                                        type="checkbox"
                                                        value="Yes"
                                                        class="s_website_form_input form-check-input"
                                                        name="code_of_honour"
                                                        id="oc8pzs7wgrmm"
                                                        required=""
                                                    />
                                                </div>
                                                <div
                                                    class="s_website_form_field_description small form-text text-muted"
                                                >
                                                    <a
                                                        href="https://www.svsmf.ch/vereinigung/ehrenkodex"
                                                        target="_blank"
                                                    >Ehrenkodex</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div
                                        data-name="Field"
                                        class="s_website_form_field mb-3 col-12 s_website_form_custom s_website_form_required"
                                        data-type="boolean"
                                        data-translated-name="Feld"
                                    >
                                        <div
                                            class="row s_col_no_resize s_col_no_bgcolor"
                                        >
                                            <label
                                                class="col-sm-auto s_website_form_label"
                                                style="width: 200px"
                                                for="omq7wgqig0r"
                                            >
                                                <span
                                                    class="s_website_form_label_content"
                                                >Ich habe die Strahlnerverordnung gelesen</span>
                                                <span
                                                    class="s_website_form_mark"
                                                > *</span>
                                            </label>
                                            <div class="col-sm">
                                                <div class="form-check">
                                                    <input
                                                        type="checkbox"
                                                        value="Yes"
                                                        class="s_website_form_input form-check-input"
                                                        name="strahlner_ordinance"
                                                        required="1"
                                                        id="omq7wgqig0r"
                                                    />
                                                </div>
                                                <div
                                                    class="s_website_form_field_description small form-text text-muted"
                                                >
                                                    <a
                                                        href="/web/content/869?unique=6c4e7a66eef6ced34f0ff27bd5a089665e890b0e&amp;download=true"
                                                        target="_blank"
                                                    >Strahlnerverordnung</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div
                                        data-name="Field"
                                        class="s_website_form_field mb-3 col-12 s_website_form_custom s_website_form_required"
                                        data-type="boolean"
                                        data-translated-name="Feld"
                                    >
                                        <div
                                            class="row s_col_no_resize s_col_no_bgcolor"
                                        >
                                            <label
                                                class="col-sm-auto s_website_form_label"
                                                style="width: 200px"
                                                for="ozd0x2j7f0d"
                                            >
                                                <span
                                                    class="s_website_form_label_content"
                                                >Ich bin 14 Jahre oder älter</span>
                                                <span
                                                    class="s_website_form_mark"
                                                > *</span>
                                            </label>
                                            <div class="col-sm">
                                                <div class="form-check">
                                                    <input
                                                        type="checkbox"
                                                        value="Yes"
                                                        class="s_website_form_input form-check-input"
                                                        name="minimum_age"
                                                        id="ozd0x2j7f0d"
                                                        required=""
                                                    />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div
                                        class="s_website_form_submit s_website_form_no_submit_option d-flex flex-column flex-lg-row align-items-lg-center pt-4"
                                    >
                                        <a
                                            role="button"
                                            name="website_sale_main_button"
                                            class="s_website_form_send btn btn-primary order-lg-3 w-100 w-lg-auto ms-lg-auto"
                                            href="/shop/confirm_order"
                                        > Kassiervorgang fortsetzen <i
                                            class="fa fa-angle-right ms-2 fw-light"
                                        />
                                        </a>

                                        <div
                                            class="position-relative d-flex d-lg-none w-100 justify-content-center align-items-center my-2 opacity-75"
                                        >
                                            <hr class="w-100" />
                                            <span class="px-3">oder</span>
                                            <hr class="w-100" />
                                        </div>

                                        <a href="/shop/checkout" class="text-center">
                                            <i
                                            class="fa fa-angle-left me-2 fw-light"
                                        /> Zurück zum
                                            Versand </a>
                                        <span id="s_website_form_result" />
                                    </div>
                                </div>
                            </form>
                        </div>
                    </section>
                </t>
            </t>
        </field>
    </record>
</odoo>
