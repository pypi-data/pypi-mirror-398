{
  "$comment": "Parity test vectors for ServiceHash - ensures Python SDK produces same hashes as TypeScript SDK",
  "$note": "Both SDKs use INSERTION ORDER (not sorted keys): service, input, version, timestamp. Null inputs are omitted.",
  "test_vectors": [
    {
      "description": "Simple service with string input",
      "input": {
        "service": "gpt-4-turbo",
        "input": "Hello, world!",
        "version": "1.0.0",
        "timestamp": 1700000000
      },
      "expected_canonical": "{\"service\":\"gpt-4-turbo\",\"input\":\"Hello, world!\",\"version\":\"1.0.0\",\"timestamp\":1700000000}",
      "expected_hash": "0xab39b249750272b9aa3289e5983ebe2316d47b0d0ce29cb52ce02e344442dd13"
    },
    {
      "description": "Service with object input",
      "input": {
        "service": "dalle-3",
        "input": {"prompt": "A cat", "size": "1024x1024"},
        "version": "2.0.0"
      },
      "expected_canonical": "{\"service\":\"dalle-3\",\"input\":{\"prompt\":\"A cat\",\"size\":\"1024x1024\"},\"version\":\"2.0.0\"}",
      "expected_hash": "0xce57955961a4a5e7e0d1fb31930d75f53f404f7fd9a187eeb4fddc59204d7127"
    },
    {
      "description": "Minimal service (service name only)",
      "input": {
        "service": "simple-service"
      },
      "expected_canonical": "{\"service\":\"simple-service\"}",
      "expected_hash": "0x2938f38aec404b340f27dec4d504c0a1e20cf02942971fb63adec84a195c9ef6"
    },
    {
      "description": "Service with null input (should be omitted)",
      "note": "Null inputs are OMITTED from canonical JSON (matching TS SDK spread operator behavior)",
      "input": {
        "service": "test-service",
        "input": null,
        "version": "1.0.0"
      },
      "expected_canonical": "{\"service\":\"test-service\",\"version\":\"1.0.0\"}",
      "expected_hash": "0x13aa8e0b9138cb73a124d6c8b85207fc1231c9e0ed6192f5352f085fe4429809"
    },
    {
      "description": "Service with numeric input",
      "input": {
        "service": "calc",
        "input": 42,
        "timestamp": 1700000000
      },
      "expected_canonical": "{\"service\":\"calc\",\"input\":42,\"timestamp\":1700000000}",
      "expected_hash": "0x47039a960853ebaf64bc351787824d136c8dacd362f2b1c70bfb6b8ae6a61fe8"
    },
    {
      "description": "Service with array input",
      "input": {
        "service": "batch-process",
        "input": ["item1", "item2", "item3"]
      },
      "expected_canonical": "{\"service\":\"batch-process\",\"input\":[\"item1\",\"item2\",\"item3\"]}",
      "expected_hash": "0xdde96fad018bc7d286b03c4d1acc43341873b5b09928e2d50af95a188271eccb"
    },
    {
      "description": "Service with nested object input",
      "input": {
        "service": "complex-service",
        "input": {
          "user": {"id": 123, "name": "Alice"},
          "config": {"enabled": true, "options": ["a", "b"]}
        }
      },
      "expected_canonical": "{\"service\":\"complex-service\",\"input\":{\"user\":{\"id\":123,\"name\":\"Alice\"},\"config\":{\"enabled\":true,\"options\":[\"a\",\"b\"]}}}",
      "expected_hash": "0x28195c4fb679613503ad1a07e6fa645dcd50d6692145f87a183484a1f70e933f"
    },
    {
      "description": "PARITY CRITICAL: Insertion order (service first, NOT alphabetical)",
      "note": "Keys follow INSERTION ORDER: service, input, version, timestamp (not alphabetically sorted)",
      "input": {
        "service": "order-test",
        "input": "data",
        "version": "1.0.0",
        "timestamp": 1700000000
      },
      "expected_canonical": "{\"service\":\"order-test\",\"input\":\"data\",\"version\":\"1.0.0\",\"timestamp\":1700000000}",
      "not_expected_canonical": "{\"input\":\"data\",\"service\":\"order-test\",\"timestamp\":1700000000,\"version\":\"1.0.0\"}",
      "expected_hash": "0x8bf856669d1343d72db3fa592d7dd77447fef5380b09e0ee7aa39b99fc93898a"
    },
    {
      "description": "Unicode preservation test",
      "note": "Both SDKs preserve unicode characters (no escaping)",
      "input": {
        "service": "unicode-test",
        "input": "Hello ‰∏ñÁïå üåç"
      },
      "expected_canonical": "{\"service\":\"unicode-test\",\"input\":\"Hello ‰∏ñÁïå üåç\"}",
      "expected_hash": "0x7bf88fd82f247b59a708e64e91f372ab08cf529635ade28a48c7dfb5ed6cd593"
    }
  ]
}
