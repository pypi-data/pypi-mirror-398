{% set linked_data=[
    resource.breadcrumbs.dump_linked_data(project)
] %}
{% if resource.resource is linked_data_dumpable %}
    {% do linked_data.append(resource.resource.dump_linked_data(project)) %}
{% endif %}
{% if linked_data %}
    <script type="application/ld+json">
        {{ linked_data | tojson }}
    </script>
{% endif %}
