{% if resource.resource is has_links %}
    {% if resource.resource.links | length > 0 %}
        <div class="container my-5">
            <section id="external-links" class="section">
                <h2 class="section-heading">
                    {% trans %}External links{% endtrans %}
                    {% with url = "#external-links" %}
                        {% include 'component/permalink.html.j2' %}
                    {% endwith %}
                </h2>
                {% with links = resource.resource.links %}
                    {% include 'component/external-links.html.j2' %}
                {% endwith %}
            </section>
        </div>
    {% endif %}
{% endif %}
