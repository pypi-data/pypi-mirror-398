{%- set embedded = embedded | default(False) -%}
{%- set source_context = resource.entity_contexts['source'] -%}
<div class="small">
    <p class="m-0">{{ entity.plugin().label | localize | html_lang }}</p>
    {%- set source_label -%}
        {%- with source = entity.source -%}
            {% include 'entity/source-containment-label.html.j2' %}
        {%- endwith -%}
    {%- endset -%}
    {%- trans source = source_label -%}
        in {{ source }}
    {%- endtrans -%}
</div>
