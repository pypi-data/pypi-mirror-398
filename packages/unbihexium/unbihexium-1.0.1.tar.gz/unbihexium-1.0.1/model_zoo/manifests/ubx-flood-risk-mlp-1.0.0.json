{
    "model_id": "ubx-flood-risk-mlp-1.0.0",
    "name": "Flood Risk Scorer",
    "version": "1.0.0",
    "task": "risk_scoring",
    "architecture": "mlp",
    "license": "Apache-2.0",
    "status": "production",
    "description": "MLP-based flood risk scoring model. Takes terrain and environmental features as input and outputs a risk score.",
    "input": {
        "type": "tabular",
        "features": [
            "elevation",
            "slope",
            "distance_to_water",
            "precipitation",
            "soil_type",
            "land_cover",
            "drainage",
            "population",
            "infrastructure",
            "historical_flood"
        ]
    },
    "output": {
        "type": "scalar"
    },
    "artifacts": {
        "tiny": {
            "files": [
                {
                    "name": "model.onnx",
                    "sha256": "placeholder",
                    "size_bytes": 0
                },
                {
                    "name": "config.json",
                    "sha256": "placeholder",
                    "size_bytes": 0
                }
            ]
        }
    },
    "metrics": {
        "rmse": 0.12,
        "r2": 0.85
    },
    "training": {
        "dataset": "synthetic",
        "epochs": 10,
        "batch_size": 64,
        "optimizer": "adam",
        "learning_rate": 0.001
    },
    "capabilities": [
        "flood_risk"
    ]
}