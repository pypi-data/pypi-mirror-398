{
    "model_id": "ubx-seg-multiclass-unet-1.0.0",
    "name": "Multi-class Segmentation UNet",
    "version": "1.0.0",
    "task": "segmentation",
    "architecture": "unet",
    "license": "Apache-2.0",
    "status": "production",
    "description": "UNet-based semantic segmentation model for multi-class land cover classification. Supports water, vegetation, urban, and bare soil classes.",
    "input": {
        "type": "image",
        "channels": 3,
        "height": 64,
        "width": 64,
        "normalization": {
            "mean": [
                0.5,
                0.5,
                0.5
            ],
            "std": [
                0.5,
                0.5,
                0.5
            ]
        }
    },
    "output": {
        "type": "mask",
        "classes": [
            "background",
            "water",
            "vegetation",
            "urban",
            "bare_soil"
        ]
    },
    "artifacts": {
        "tiny": {
            "files": [
                {
                    "name": "model.onnx",
                    "sha256": "placeholder",
                    "size_bytes": 0
                },
                {
                    "name": "config.json",
                    "sha256": "placeholder",
                    "size_bytes": 0
                },
                {
                    "name": "labels.json",
                    "sha256": "placeholder",
                    "size_bytes": 0
                }
            ]
        }
    },
    "metrics": {
        "iou": 0.72,
        "dice": 0.81,
        "accuracy": 0.89
    },
    "training": {
        "dataset": "synthetic",
        "epochs": 5,
        "batch_size": 32,
        "optimizer": "adam",
        "learning_rate": 0.001
    },
    "capabilities": [
        "water_segmentation",
        "crop_segmentation",
        "greenhouse_detection",
        "landcover_classification"
    ]
}