{
    "model_id": "ubx-change-siamese-1.0.0",
    "name": "Siamese Change Detector",
    "version": "1.0.0",
    "task": "change_detection",
    "architecture": "siamese",
    "license": "Apache-2.0",
    "status": "production",
    "description": "Siamese network for change detection between two temporal images. Detects changes using feature difference and produces binary change mask.",
    "input": {
        "type": "image_pair",
        "channels": 3,
        "height": 64,
        "width": 64
    },
    "output": {
        "type": "mask",
        "classes": [
            "no_change",
            "change"
        ]
    },
    "artifacts": {
        "tiny": {
            "files": [
                {
                    "name": "model.onnx",
                    "sha256": "placeholder",
                    "size_bytes": 0
                },
                {
                    "name": "config.json",
                    "sha256": "placeholder",
                    "size_bytes": 0
                },
                {
                    "name": "labels.json",
                    "sha256": "placeholder",
                    "size_bytes": 0
                }
            ]
        }
    },
    "metrics": {
        "iou": 0.68,
        "f1": 0.75,
        "precision": 0.78,
        "recall": 0.72
    },
    "training": {
        "dataset": "synthetic",
        "epochs": 5,
        "batch_size": 32,
        "optimizer": "adam",
        "learning_rate": 0.001
    },
    "capabilities": [
        "change_detection"
    ]
}